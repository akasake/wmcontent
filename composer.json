{
    "name": "wieni/wmcontent",
    "type": "drupal-module",
    "description": "Adds configurable content 'containers' to entity types (for eg.paragraphs)",
    "license": "MIT",
    "authors": [
        {
            "name": "Wieni",
            "email": "info@wieni.be"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "drupal/core": "^8.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "wieni/wmcodestyle": "^1.0",
        "ergebnis/composer-normalize": "^2.0"
    },
    "scripts": {
        "coding-standards": [
            "@composer normalize",
            "php-cs-fixer fix --config=.php_cs.php"
        ]
    }
}
